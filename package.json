{
    "name": "borderline-ui",
    "version": "0.0.5",
    "description": "Storyline-based interface for translation bioinformatics",
    "author": "Florian Guitton <f.guitton@imperial.ac.uk>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-icl/borderline-ui.git"
    },
    "bugs": {
        "url": "https://github.com/dsi-icl/borderline-ui/issues"
    },
    "homepage": "https://github.com/dsi-icl/borderline-ui#readme",
    "keywords": [
        "borderline",
        "bioinformatics",
        "transmart",
        "i2b2"
    ],
    "engines": {
        "node": ">= 7.2.0",
        "npm": ">= 4.1.0"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development node ./tools/server.js",
        "prod": "cross-env NODE_ENV=production node ./tools/server.js",
        "build": "cross-env NODE_ENV=production node ./tools/build.js",
        "lint": "eslint ./config ./src ./tools -f table || true",
        "export": "node ./tools/publish.js",
        "publish": "node ./tools/blocker.js && exit 1",
        "prepublishOnly": "node ./tools/blocker.js && exit 1"
    },
    "devDependencies": {
        "babel-core": "6.22.1",
        "babel-eslint": "7.1.1",
        "babel-loader": "6.2.10",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-preset-es2015": "6.22.0",
        "babel-preset-react": "6.22.0",
        "babel-preset-stage-0": "6.22.0",
        "borderline-server": "0.0.4",
        "css-loader": "0.26.1",
        "eslint": "3.14.1",
        "eslint-loader": "1.6.1",
        "eslint-plugin-react": "6.9.0",
        "express": "4.14.0",
        "html-loader": "0.4.4",
        "html-webpack-plugin": "2.26.0",
        "postcss-cssnext": "2.9.0",
        "postcss-import": "9.1.0",
        "postcss-loader": "1.2.2",
        "react-history": "0.18.2",
        "style-loader": "0.13.1",
        "webpack": "2.2.0",
        "webpack-dev-middleware": "1.9.0",
        "webpack-hot-middleware": "2.15.0"
    },
    "dependencies": {
        "immutable": "3.8.1",
        "react": "15.4.2",
        "react-dom": "15.4.2",
        "react-hot-loader": "3.0.0-beta.6",
        "react-redux": "5.0.2",
        "react-router": "4.0.0-alpha.6",
        "redux": "3.6.0",
        "redux-logger": "2.7.4",
        "redux-observable": "0.13.0",
        "rxjs": "5.0.3"
    }
}
