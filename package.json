    "dependencies": {
        "dompurify": "0.8.5",
        "immutable": "3.8.1",
        "react": "15.4.2",
        "react-dom": "15.4.2",
        "react-redux": "5.0.2",
        "react-router": "4.0.0-beta.6",
        "react-router-dom": "4.0.0-beta.6",
        "redux": "3.6.0",
        "redux-observable": "0.13.0",
        "rxjs": "5.1.1"
