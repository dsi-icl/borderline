{
    "name": "borderline-ui",
    "version": "0.1.1",
    "description": "Storyline-based interface for translation bioinformatics",
    "author": "Florian Guitton <f.guitton@imperial.ac.uk>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-icl/borderline-ui.git"
    },
    "bugs": {
        "url": "https://github.com/dsi-icl/borderline-ui/issues"
    },
    "homepage": "https://github.com/dsi-icl/borderline-ui#readme",
    "keywords": [
        "borderline",
        "bioinformatics",
        "transmart",
        "i2b2",
        "ui"
    ],
    "engines": {
        "node": ">= 7.2.0",
        "npm": ">= 4.1.0"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development node ./tools/server.js",
        "prod": "cross-env NODE_ENV=production node ./tools/server.js",
        "build": "cross-env NODE_ENV=production node ./tools/build.js",
        "lint": "eslint ./config ./src ./tools -f table || true",
        "export": "node ./tools/publish.js",
        "publish": "node ./tools/blocker.js && exit 1",
        "prepublishOnly": "node ./tools/blocker.js && exit 1"
    },
    "devDependencies": {
        "babel-core": "6.23.1",
        "babel-eslint": "7.1.1",
        "babel-loader": "6.3.2",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-preset-es2015": "6.22.0",
        "babel-preset-react": "6.23.0",
        "babel-preset-stage-0": "6.22.0",
        "borderline-server": "0.1.4",
        "cross-env": "3.1.4",
        "css-loader": "0.26.1",
        "eslint": "3.15.0",
        "eslint-loader": "1.6.1",
        "eslint-plugin-react": "6.10.0",
        "express": "4.14.1",
        "file-loader": "0.10.0",
        "html-loader": "0.4.4",
        "html-webpack-plugin": "2.28.0",
        "postcss-cssnext": "2.9.0",
        "postcss-import": "9.1.0",
