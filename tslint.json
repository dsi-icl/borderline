{
    "extends": [
        "tslint:latest"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules",
            "build"
        ]
    },
    "rules": {
        "object-literal-sort-keys": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": false,
        "indent": [
            true,
            "spaces",
            4
        ]
    }
}